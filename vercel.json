{
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200,
      "has": [
        {
          "type": "header",
          "key": "authorization",
          "value": "Basic fra:antelao"
        }
      ]
    },
    {
      "src": "/.*",
      "status": 401,
      "headers": { "www-authenticate": "Basic realm=\"www\"" },
      "missing": [
        {
          "type": "header",
          "key": "authorization",
          "value": "Basic fra:antelao"
        }
      ]
    }
  ]
}
